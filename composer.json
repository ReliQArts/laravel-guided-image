{
  "name": "reliqarts/laravel-guided-image",
  "description": "Simplified and ready image manipulation for Laravel 5 via intervention image.",
  "keywords": [
    "image",
    "route",
    "generation",
    "laravel",
    "photo",
    "laravel5",
    "resize",
    "thumb",
    "dummy",
    "crop"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "reliq",
      "email": "reliq@reliqarts.com"
    }
  ],
  "replace": {
    "reliqarts/guided-image": "1.*"
  },
  "require": {
    "php": "^7.2",
    "illuminate/support": "^5.8|^6.0",
    "intervention/image": "^2.4",
    "intervention/imagecache": "^2.3",
    "reliqarts/laravel-common": "^2.0@dev",
    "ext-json": "*"
  },
  "require-dev": {
    "codeception/aspect-mock": "^3.0",
    "orchestra/testbench": "^3.8",
    "phpro/grumphp": "^0.15.2",
    "phpunit/phpunit": "^8.1",
    "wearejust/grumphp-extra-tasks": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "ReliqArts\\GuidedImage\\": "src/",
      "ReliqArts\\GuidedImage\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "ReliqArts\\GuidedImage\\ServiceProvider"
      ]
    }
  }
}
